    "emoji-picker-react": "^4.7.18",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "file-type": "^19.0.0",
    "matter-js": "^0.19.0",
    "nestjs-prisma": "^0.23.0",
    "passport": "^0.7.0",
    "passport-42": "^1.2.6",
    "phaser": "^3.70.0",
    "phaser3spectorjs": "^0.0.8",
    "react-input-emoji": "^5.6.6",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2",
    "twilio": "^4.22.0"
